mod test

extern func printf(fmt *byte, ...)

func main() double {
	let j mut int, _t = 15
	for let i mut = 0; i <= 20; i += 5 {
		switch i {
			// j = 1
		case 0:
			j = 5
			// fallthrough
		case 10, 15:
			j = 10
		default:
			j = -1
		case 20:
			j = 19
		}
		printf("%d => %d\n", i, j)
	}
	// break
	return j
}
